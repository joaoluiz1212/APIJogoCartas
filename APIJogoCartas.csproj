    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>76655517-a0e4-4b2c-8daf-79bdeeba78db</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
